{
  "name": "solid-admin",
  "version": "0.0.0",
  "description": "",
  "author": {
    "name": "thunur"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "preview": "pnpm run build && vite preview --host",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "eslint ./src --ext .vue,.js,jsx,.ts,tsx",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,jsx,.ts,tsx",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lock": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json",
    "clean:lib": "rimraf node_modules",
    "commit": "npx cz",
    "clean": "pnpm clean:lock && pnpm clean:lib"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.18",
    "@xbmlz/eslint-config-prettier": "^0.1.16",
    "@xbmlz/eslint-config-solid": "^0.1.16",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "less": "^4.1.3",
    "rimraf": "^3.0.2",
    "vite": "^4.0.3",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-solid": "^2.5.0",
    "vite-plugin-windicss": "^1.8.10"
  },
  "dependencies": {
    "@solidjs/router": "^0.6.0",
    "axios": "^1.2.2",
    "mockjs": "^1.1.0",
    "solid-js": "^1.6.6"
  }
}
